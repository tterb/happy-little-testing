{
  "name": "joy-of-testing",
  "description": "A website for testing and experimenting with published data",
  "version": "1.0.0",
  "author": "Brett Stevenson <bstevensondev@gmail.com> (https://brettstevenson.io)",
  "homepage": "https://joy-of-testing.netlify.com",
  "license": "MIT",
  "scripts": {
    "build": "yarn build-plugin && gatsby build",
    "build-plugin": "yarn --cwd ./plugins/gatsby-plugin-disqus && yarn --cwd ./plugins/gatsby-plugin-disqus build",
    "bootstrap": "yarn && yarn build-plugin",
    "develop": "gatsby develop --port 4000",
    "local": "GATSBY_ACTIVE_ENV=local yarn develop",
    "backend": "GATSBY_ACTIVE_ENV=backend yarn develop",
    "production": "yarn build && gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml"
  },
  "keywords": [
    "joy",
    "testing",
    "bob-ross",
    "paintings",
    "disqus",
    "blog",
    "gatsby"
  ],
